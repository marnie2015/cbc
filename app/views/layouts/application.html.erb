<!DOCTYPE html>
<html>
  <head>
    <title>Crossroad Baptist Church</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    
  </head>

  <body>
    <nav>
      <div class="nav-wrapper">
        <a href="#" class="brand-logo c-yellow"><img src="/assets/logo.jpg" width="180px;"></a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">
          <li><a class="c-yellow" href="#">PUBLICATIONS</a></li>
          <li><a class="c-yellow" href="#">HEAVEN</a></li>
          <li><a class="c-yellow" href="#">CONTACT US</a></li>
        </ul>
      </div>
    </nav>
    <%= yield %>
  </body>
</html>
